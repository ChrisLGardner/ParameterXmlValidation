{
  "manifestVersion": 1,
  "id": "CLG-ParameterXmlValidation",
  "version": "1.0.0",
  "name": "Parameter XML Validation",
  "publisher": "chrisgardner",
  "description": "Task validating Parameters.xml files correctly match the web.config, app.config, and unity.config files it references.",
  "categories": [
    "Azure Pipelines"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "tags": [
    "Validation",
    "web.config",
    "app.config",
    "unity.config",
    "parameter",
    "app",
    "unity",
    "web"
  ],
  "galleryFlags": [
    "Public"
  ],
  "branding": {
    "color": "#3116cc",
    "theme": "dark"
  },
  "icons": {
    "default": "images/logo.png"
  },
  "links": {
    "getstarted": {
      "uri": "https://github.com/ChrisLGardner/ParameterXmlValidation"
    },
    "support": {
      "uri": "https://github.com/ChrisLGardner/ParameterXmlValidation/issues"
    }
  },
  "screenshots": [],
  "content": {
    "details": {
      "path": "readme.md"
    },
    "license": {
      "path": "license.md"
    }
  },
  "files": [
    {
      "path": "ParameterValidate"
    }
  ],
  "contributions": [
    {
      "id": "ParameterXmlValidation-Task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "ParameterValidate"
      }
    }
  ]
}
